{
  "name": "immutable-custom-merge",
  "version": "0.2.1",
  "description": "Custom deep merging of Immutable data structures",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/ test/",
    "start": "babel-node --presets es2015 ./src/index.js",
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "babel src -d dist"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "dist",
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruzicka/immutable-custom-merge.git"
  },
  "keywords": [
    "immutable",
    "immutablejs",
    "merge",
    "custom"
  ],
  "author": "David Ruzicka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruzicka/immutable-custom-merge/issues"
  },
  "homepage": "https://github.com/ruzicka/immutable-custom-merge#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "immutable": "^3.8.1",
    "jest": "^19.0.2",
    "mocha": "^3.2.0"
  },
  "peerDependencies": {
    "immutable": "^3.8.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
